          ID : {clubs.NumClub}<br></br>\n                  Nom : {clubs.NomClub}<br></br>\n                </p>\n\n              </Popup>\n            </Marker>\n          );\n        })}\n      </MarkerClusterGroup>\n      \n     \n    </MapContainer>\n    </div> \n    \n  );\n}\n\n\n\nexport default Home;\n","/home/manuel/ClubsRegion/front/src/components/filters/Filter.jsx",["64"],"/home/manuel/ClubsRegion/front/src/App.jsx",["65","66","67"],"/home/manuel/ClubsRegion/front/src/components/loader/Loader.jsx",[],"/home/manuel/ClubsRegion/front/src/index.jsx",[],["68","69"],"/home/manuel/ClubsRegion/front/src/Fetch.js",[],"/home/manuel/ClubsRegion/front/src/components/header/Header.jsx",["70"],"/home/manuel/ClubsRegion/front/src/components/Hook/useGeolocation.js",[],"/home/manuel/ClubsRegion/front/src/components/PopUp/Modal.js",[],"/home/manuel/ClubsRegion/front/src/components/PopUp/UsePopup.js",["71","72"],"/home/manuel/ClubsRegion/front/src/components/Club/Club.jsx",["73","74","75","76"],{"ruleId":"77","severity":1,"message":"78","line":22,"column":20,"nodeType":"79","messageId":"80","endLine":22,"endColumn":31},{"ruleId":"77","severity":1,"message":"81","line":26,"column":22,"nodeType":"79","messageId":"80","endLine":26,"endColumn":35},{"ruleId":"77","severity":1,"message":"82","line":27,"column":10,"nodeType":"79","messageId":"80","endLine":27,"endColumn":16},{"ruleId":"77","severity":1,"message":"83","line":28,"column":10,"nodeType":"79","messageId":"80","endLine":28,"endColumn":16},{"ruleId":"77","severity":1,"message":"84","line":29,"column":10,"nodeType":"79","messageId":"80","endLine":29,"endColumn":16},{"ruleId":"77","severity":1,"message":"85","line":30,"column":10,"nodeType":"79","messageId":"80","endLine":30,"endColumn":16},{"ruleId":"77","severity":1,"message":"86","line":31,"column":10,"nodeType":"79","messageId":"80","endLine":31,"endColumn":16},{"ruleId":"77","severity":1,"message":"87","line":31,"column":18,"nodeType":"79","messageId":"80","endLine":